<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Home page</h1>
    <h3><%= message %></h3>
    <table width="100%" border="1">
        <tr>
            <th>NO</th>
            <th>NAME</th>
            <th>PRICE</th>
            <th>COMPANY</th>
            <th>YEAR</th>
        </tr>
        <% carList.forEach(function(car) { %>
            <tr>
                <td><%=car.no%></td>
                <td><%=car.name%></td>
                <td><%=car.price%></td>
                <td><%=car.company%></td>
                <td><%=car.year%></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>